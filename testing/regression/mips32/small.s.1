entry:
  [%add] <- "ADDU $%add, $%a, $%a" <- [%a, %a]
  [%cmp] <- "SLTi %cmp, %add, 10" <- [%add]
  [] <- "BNE $0, $%cmp, bb0" <- [%cmp]
bb1:
if.end:
  [%add.copy.2] <- "MOV %add.copy.2, %add" <- [%add]
  [] <- "B return" <- []
bb0:
if.then:
  [%const.10.copy.2] <- "ADDiu %const.10.copy.2, $0, 10" <- []
return:
  [%retval.0] <- "PHI (%const.10.copy.2, if.then) (%add.copy.2, if.end)" <- [%const.10.copy.2, %add.copy.2]
  [] <- "RetRA %retval.0" <- [%retval.0]
