entry:
  %const.-1.copy.1 = ##-1
  jump for.cond
bb0:
for.body:
for.inc:
  %mul = mpy(%n.addr.0, %res.0)
  %dec = add(%n.addr.0, %const.-1.copy.1)
for.cond:
  %res.0 = PHI (%n, entry) (%mul, for.inc)
  %n.addr.0 = PHI (%n, entry) (%dec, for.inc)
  %cmp = cmp.gt(%n.addr.0, 1)
  if (%cmp) jump bb0
bb1:
for.end:
  return %res.0
